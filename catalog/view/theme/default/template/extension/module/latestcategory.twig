<div class="row">
 {% for latestcategory in latestcategorys %}
  <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6" style="margin-bottom: 15px">
    <div class="product-thumb transition">
      <div class="image"><a href="{{ latestcategory.href }}"><img src="{{ latestcategory.thumb }}" alt="{{ latestcategory.name }}" title="{{ latestcategory.name }}" class="img-responsive" /></a></div>
      <div class="caption">
        <h4 style="text-align: center"><a href="{{ latestcategory.href }}">{{ latestcategory.name }}</a></h4>
      </div>
    </div>
  </div>
  {% endfor %}
</div>
